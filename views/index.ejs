<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

<body>

    <%- include('./partials/nav.ejs') %>

    <h1>All Blogs</h1>
    <p>This is the Home page</p>

    <% if(blogs.length > 0) { %>
        <% blogs.forEach(blog => { %>
            <h3 class="title" > <%= blog.title %> </h3>
            <p class="snippet" ><%= blog.snippet %></p>    
        <% }) %>
    <% } else { %>
        <p>There are no blogs</p>
    <% } %>

    <%- include('./partials/footer.ejs) %>
</body>
</html>